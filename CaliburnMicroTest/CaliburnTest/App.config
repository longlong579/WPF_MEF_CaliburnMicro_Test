<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net-net-1.0" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <add key="SocketIP" value="127.0.0.1"/>
    <add key="SocketPort" value="2020"/>
    <add key="CommandTimeout" value="120" />
    <add key="NetworkConnectCount" value="5" />
    <add key="NetworkConnectTimeout" value="10" />
  </appSettings>
</configuration>